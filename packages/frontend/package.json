    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.1",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4"
  },
  "type": "module",
  "dependencies": {
    "@googleapis/drive": "^2.0.2",
    "archieml": "^0.5.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
